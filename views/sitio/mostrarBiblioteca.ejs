<%- include("../../templates/encabezado")%>
<%- include("../../templates/menuadmin")%>
<div class="container mt-5">
  <div class="card">
    <div class="card-header">
      <h1>Juegos</h1>
    </div>
    <div class="card-body">
      <table class="table">
        <thead>
          <tr>
            <th>Id</th>
            <th>Titulo</th>
            <th>Correo</th>
            <th>MÃ©todo de pago</th>
            <th>Identificacion</th>
            <th>Editar/Eliminar</th>
          </tr>
        </thead>
        <tbody>
          <% negocios.forEach((negocio, i) => { %>
            <tr>
              <td><%= i + 1 %></td>
              <td><%= negocio.titulo %></td>
              <td><%= negocio.correo %></td>
              <td><%= negocio.metodopago %></td>
              <td><img src="/Negocios/images/<%= negocio.identificacion %>" width="100px"></td>
              <td>
                <a href="/editarnegocio/<%=negocio.id%>">Editar</a>
                <a href="/borrarnegocioadmin/<%=negocio.id%>">Borrar</a>
              </td>
            </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
    <div class="card-footer"></div>
  </div>
</div>
